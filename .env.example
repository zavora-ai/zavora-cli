# Runtime defaults
ZAVORA_PROVIDER=auto
ZAVORA_MODEL=
ZAVORA_APP_NAME=zavora-cli
ZAVORA_USER_ID=local-user
ZAVORA_SESSION_ID=default-session
ZAVORA_SESSION_BACKEND=memory
ZAVORA_SESSION_DB_URL=sqlite://.zavora/sessions.db
ZAVORA_GUARDRAIL_INPUT_MODE=disabled
ZAVORA_GUARDRAIL_OUTPUT_MODE=disabled
ZAVORA_GUARDRAIL_TERM=
ZAVORA_GUARDRAIL_REDACT_REPLACEMENT=[REDACTED]

# Provider credentials (set at least one for cloud models)
GOOGLE_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
DEEPSEEK_API_KEY=
GROQ_API_KEY=

# Optional local provider endpoint
OLLAMA_HOST=http://localhost:11434
